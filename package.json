    "aws-serverless-express": "^3.4.0",
    "axios": "^1.6.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "docusign-esign": "^7.0.1",
    "dotenv": "^16.3.2",
    "express-mongo-sanitize": "^2.2.0",
    "firebase-admin": "^12.6.0",
    "helmet": "^7.1.0",
    "moment": "^2.30.1",
    "mongoose": "^8.1.0",
    "morgan": "^1.10.0",
