    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.4",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/websockets": "^10.3.4",
    "@types/axios": "^0.14.0",
    "@types/moment": "^2.13.0",
